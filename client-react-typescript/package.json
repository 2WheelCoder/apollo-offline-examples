{
  "name": "apollo-offline-examples",
  "version": "0.0.0",
  "description": "Examples for apollo-offline",
  "homepage": "https://github.com/Malpaux/apollo-offline-examples#readme",
  "bugs": {
    "url": "https://github.com/Malpaux/apollo-offline-examples"
  },
  "license": "BSD-3-Clause",
  "private": true,
  "author": "Malpaux (malpaux.com)",
  "contributors": [
    {
      "name": "Paul Brachmann",
      "email": "paul.brachmann@malpaux.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Malpaux/apollo-offline-examples.git"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "lint": "tslint -p tsconfig.json 'src/**/*.ts' 'src/**/*.tsx'",
    "lint:fix": "tslint -p tsconfig.json --fix 'src/**/*.ts' 'src/**/*.tsx'",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:coverage": "node scripts/test.js --env=jsdom --coverage"
  },
  "dependencies": {
    "apollo-offline": "^0.0.5",
    "csx": "^8.4.0",
    "react": "^15.6.1",
    "react-apollo": "^1.4.14",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.0",
    "react-typestyle": "^0.3.0",
    "react-typestyle-inline": "^0.3.1",
    "react-typestyle-preset": "^0.3.1",
    "redux": "^3.7.2",
    "redux-offline": "^2.0.0",
    "theming": "^1.1.0",
    "typestyle": "^1.3.8",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.6",
    "@types/jest": "^20.0.8",
    "@types/node": "^8.0.24",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-redux": "^5.0.5",
    "@types/react-router-dom": "^4.0.7",
    "@types/redux": "^3.6.0",
    "@types/webpack-env": "^1.13.0",
    "awesome-typescript-loader": "^3.2.3",
    "babel-polyfill": "^6.26.0",
    "browserconfig-loader": "^0.0.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.1.0",
    "enzyme": "^2.9.1",
    "extract-loader": "^1.0.1",
    "file-loader": "^0.11.2",
    "fs-extra": "^4.0.1",
    "graphql-tag": "^2.4.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "img-loader": "^2.0.0",
    "jest": "^20.0.4",
    "jest-localstorage-mock": "^1.2.0",
    "jest-transform-graphql": "^2.1.0",
    "react-dev-utils": "^3.1.1",
    "react-error-overlay": "^1.0.10",
    "react-test-renderer": "^15.6.1",
    "sw-precache-webpack-plugin": "^0.11.4",
    "ts-jest": "^20.0.10",
    "tslint": "^5.6.0",
    "tslint-config-airbnb": "^5.2.1",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.2.0",
    "typescript": "^2.4.2",
    "url-loader": "^0.5.9",
    "web-manifest-loader": "^0.0.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "mapCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "\\.gql$"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "<rootDir>/(build|dist|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost"
  }
}
